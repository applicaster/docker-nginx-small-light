nginx:
  build: .
  ports:
    - <%= @server_port %>:80
    - 443
  environment:
    UPSTREAM_BASE_URL: <%= "http://#{@server_ip}:#{@images_port}" %>
image_server:
  image: cannin/nodejs-http-server
  ports:
    - <%= @images_port %>:8080
  volumes:
    - ./spec/fixtures:/home
  command: http-server -od --cors /home